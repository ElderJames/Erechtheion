{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=(localdb)\\MSSQLLocalDB;Initial Catalog=DNIC.Erechtheion;Integrated Security=True;"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Error",
      "Override": {
        "System": "Warning",
        "Microsoft": "Warning",
        "DNIC": "Debug"
      }
    }
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "Authentication": {
    "Authority": "http://localhost:5000",
    "RequireHttpsMetadata": false,
    "ApiName": "DNIC"
  },
  "Erechtheion": {
    "AuthenticationMode": "SelfAndExternal"
  }
}